  "dependencies": {
    "@storybook/addons": "6.0.16",
    "@storybook/components": "6.0.16",
    "@storybook/core-events": "6.0.16",
    "@storybook/api": "6.0.16"
  },
  "devDependencies": {
    "@rogal/front-linter": "3.0.0-beta.6",
    "@rogal/react-translate": "1.0.0",
    "@rollup/plugin-multi-entry": "3.0.1",
